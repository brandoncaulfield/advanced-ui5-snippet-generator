{
    "name": "advanced-ui5-snippet-generator",
    "displayName": "Advanced UI5 Snippet Generator",
    "description": "A UI5 snippet generator to help speed up your UI5 development :)",
    "version": "0.0.1",
    "publisher": "BrandonCaulfield",
    "repository": {
        "type": "git",
        "url": "https://github.com/brandoncaulfield/advanced-ui5-snippet-generator"
    },
    "keywords": [
        "ui5",
        "fiori",
        "sap"
    ],
    "icon": "media/icon.png",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "json",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "xml",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
